name = "coolresume"
send_metrics = false
pages_build_output_dir = ".svelte-kit/cloudflare"
compatibility_date = "2024-04-04"

[[d1_databases]]
binding = "DB"
database_name = "coolresume_db"
database_id = "DATABASE_ID_HERE"
migrations_dir = "migrations"

[[r2_buckets]]
binding = 'BUCKET'
bucket_name = 'coolresume-bucket'

[vars]
