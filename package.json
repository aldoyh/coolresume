{
	"name": "cool-resume",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev1": "wrangler pages dev .svelte-kit/cloudflare --compatibility-flag=nodejs_compat",
		"build": "vite build",
		"preview": "vite preview",
		"proxy": "wrangler dev node_modules/cfw-bindings-wrangler-bridge/worker/index.js",
		"deploy": "npm run build && npx wrangler pages deploy .svelte-kit/cloudflare --commit-dirty=true --branch=main --env production",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"db:gen": "drizzle-kit generate:sqlite",
		"db:mig:local": "wrangler d1 migrations apply coolresume_db --local",
		"db:mig:prod": "wrangler d1 migrations apply coolresume_db --remote",
		"db:mig:preview": "wrangler d1 migrations apply coolresume_db --preview",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240402.0",
		"@iconify/json": "^2.2.197",
		"@skeletonlabs/skeleton": "2.9.0",
		"@skeletonlabs/tw-plugin": "0.3.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-cloudflare": "^4.2.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.12",
		"autoprefixer": "10.4.19",
		"better-sqlite3": "^9.4.5",
		"cfw-bindings-wrangler-bridge": "^0.10.1",
		"drizzle-kit": "^0.20.17",
		"postcss": "8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-dnd-action": "^0.9.42",
		"tailwindcss": "3.4.3",
		"typescript": "^5.0.0",
		"unplugin-icons": "^0.18.5",
		"vite": "^5.0.3",
		"vite-plugin-tailwind-purgecss": "^0.2.1",
		"vitest": "^1.2.0",
		"wrangler": "^3.42.0"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-sqlite": "^3.0.1",
		"arctic": "^1.5.0",
		"drizzle-orm": "^0.30.6",
		"form-data": "^4.0.0",
		"intro.js": "^7.2.0",
		"zod": "^3.23.4"
	}
}
